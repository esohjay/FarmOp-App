<% layout('layouts/boilerplate') %>


<div class="ui raised segments">
    <div class="ui segment green">
        <p class="ui huge header">
        <%= livestockIncome.income %>
        </p>
    </div>
    <div class="ui piled segments">
        <div class="ui yellow segment">
            <p><%= livestockIncome.date.toDateString() %>
            </p>
        </div>
        <div class="ui segment">
            <p><%= livestockIncome.amount %>
            </p>
        </div>
        <div class="ui segment">
            <p><%= livestockIncome.note %>
            </p>
        </div>
    </div>
    <div class="ui segment">
        <img class="ui centered  image" src="<%= livestockIncome.image %>">
    </div>
</div>

<a class="ui button small green" href="/report/finance/livestock">Back<i
        class=" yellow caret square right outline float right icon"></i></a>